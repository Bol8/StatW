@model IEnumerable<Repository.Liga>


<div class="col-md-2 VerticalContainer">
    @foreach (var item in Model)
    {
        <div class="col-md-12">
            <div class="thumbnail">

                <a href="@Url.Action("Index","Statistics", new { league = item.IdLiga })"
                   data-ajax-update="#ProgForm"
                   data-ajax-mode="replace"
                   data-ajax-method="GET"
                   data-ajax="true">

                    <img class="MyImage" src="@item.Imagen" alt="..." style="width:250px; max-height:150px;">
                </a>


                @*<div class="caption">
                        <h3>Thumbnail label</h3>
                        <p>...</p>
                        <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
                    </div>*@
            </div>
        </div>
    }
</div>

